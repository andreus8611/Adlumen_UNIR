   CASE 
      WHEN Vw_Preiodo10.ImpMov IS NULL THEN 0
      WHEN Vw_Preiodo10.ImpMov &gt;= 0    THEN Vw_Preiodo10.ImpMov 
   END  AS Periodo10     ,
   CASE 
      WHEN Vw_Preiodo11.ImpMov IS NULL THEN 0
      WHEN Vw_Preiodo11.ImpMov &gt;= 0    THEN Vw_Preiodo11.ImpMov 
   END  AS Periodo11     ,
   CASE 
      WHEN Vw_Preiodo12.ImpMov IS NULL THEN 0
      WHEN Vw_Preiodo12.ImpMov &gt;= 0    THEN Vw_Preiodo12.ImpMov 
   END  AS Periodo12     ,
   CASE 
