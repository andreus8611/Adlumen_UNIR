    <DataSet Name="Periodo">
      <Query>
        <DataSourceName>RsSource01</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="IdTenant">
            <Value>=Parameters!IdTenant.Value</Value>
            <rd:UserDefined>true</rd:UserDefined>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT
   Pry_Proyectos.IdProyecto                                 ,
   PRY_PERIODOSPROYECTOS.Nombre               ,
   PRY_PERIODOSPROYECTOS.FechaInicio as FecIni,
   PRY_PERIODOSPROYECTOS.FechaFin    as FecFin,
   PRY_PERIODOSPROYECTOS.Secuencia
FROM
   Pry_Proyectos 
        INNER JOIN PRY_PERIODOSPROYECTOS ON Pry_Proyectos.IdProyecto = PRY_PERIODOSPROYECTOS.IdProyecto
WHERE Pry_Proyectos.IdTenant = @IdTenant</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
