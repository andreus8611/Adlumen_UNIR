      WHEN Vw_Preiodo14.ImpMov &gt;= 0    THEN Vw_Preiodo14.ImpMov 
   END  AS Periodo14     ,
   CASE 
      WHEN Vw_Preiodo15.ImpMov IS NULL THEN 0
      WHEN Vw_Preiodo15.ImpMov &gt;= 0    THEN Vw_Preiodo15.ImpMov 
   END  AS Periodo15     ,
   CASE 
      WHEN Vw_Preiodo16.ImpMov IS NULL THEN 0
      WHEN Vw_Preiodo16.ImpMov &gt;= 0    THEN Vw_Preiodo16.ImpMov 
   END  AS Periodo16     ,
   CASE 
      WHEN Vw_Preiodo17.ImpMov IS NULL THEN 0
      WHEN Vw_Preiodo17.ImpMov &gt;= 0    THEN Vw_Preiodo17.ImpMov 
   END  AS Periodo17     ,
   CASE 
      WHEN Vw_Preiodo18.ImpMov IS NULL THEN 0
      WHEN Vw_Preiodo18.ImpMov &gt;= 0    THEN Vw_Preiodo18.ImpMov 
   END  AS Periodo18     ,
   CASE 
      WHEN Vw_Preiodo19.ImpMov IS NULL THEN 0
      WHEN Vw_Preiodo19.ImpMov &gt;= 0    THEN Vw_Preiodo19.ImpMov 
   END  AS Periodo19     ,
   CASE 
      WHEN Vw_Preiodo20.ImpMov IS NULL THEN 0
      WHEN Vw_Preiodo20.ImpMov &gt;= 0    THEN Vw_Preiodo20.ImpMov 
   END  AS Periodo20     ,   
   CASE 
      WHEN Vw_Preiodo21.ImpMov IS NULL THEN 0
      WHEN Vw_Preiodo21.ImpMov &gt;= 0    THEN Vw_Preiodo21.ImpMov 
   END  AS Periodo21     ,
   CASE 
      WHEN Vw_Preiodo22.ImpMov IS NULL THEN 0
      WHEN Vw_Preiodo22.ImpMov &gt;= 0    THEN Vw_Preiodo22.ImpMov 
   END  AS Periodo22     ,
   CASE 
      WHEN Vw_Preiodo23.ImpMov IS NULL THEN 0
      WHEN Vw_Preiodo23.ImpMov &gt;= 0    THEN Vw_Preiodo23.ImpMov 
   END  AS Periodo23     ,
   CASE 
      WHEN Vw_Preiodo24.ImpMov IS NULL THEN 0
      WHEN Vw_Preiodo24.ImpMov &gt;= 0    THEN Vw_Preiodo24.ImpMov 
   END  AS Periodo24     ,
   CASE 
      WHEN Vw_Preiodo25.ImpMov IS NULL THEN 0
      WHEN Vw_Preiodo25.ImpMov &gt;= 0    THEN Vw_Preiodo25.ImpMov 
   END  AS Periodo25     ,
   CASE 
      WHEN Vw_Preiodo26.ImpMov IS NULL THEN 0
      WHEN Vw_Preiodo26.ImpMov &gt;= 0    THEN Vw_Preiodo26.ImpMov 
   END  AS Periodo26     ,
   CASE 
      WHEN Vw_Preiodo27.ImpMov IS NULL THEN 0
      WHEN Vw_Preiodo27.ImpMov &gt;= 0    THEN Vw_Preiodo27.ImpMov 
   END  AS Periodo27     ,
   CASE 
      WHEN Vw_Preiodo28.ImpMov IS NULL THEN 0
      WHEN Vw_Preiodo28.ImpMov &gt;= 0    THEN Vw_Preiodo28.ImpMov 
   END  AS Periodo28     ,
   CASE 
      WHEN Vw_Preiodo29.ImpMov IS NULL THEN 0
      WHEN Vw_Preiodo29.ImpMov &gt;= 0    THEN Vw_Preiodo29.ImpMov 
   END  AS Periodo29     ,
   CASE 
      WHEN Vw_Preiodo30.ImpMov IS NULL THEN 0
      WHEN Vw_Preiodo30.ImpMov &gt;= 0    THEN Vw_Preiodo30.ImpMov 
   END  AS Periodo30     ,   
-- ============================================================
   CASE 
      WHEN Vw_Preiodo31.ImpMov IS NULL THEN 0
      WHEN Vw_Preiodo31.ImpMov &gt;= 0    THEN Vw_Preiodo31.ImpMov 
   END  AS Periodo31     ,
   CASE 
      WHEN Vw_Preiodo32.ImpMov IS NULL THEN 0
      WHEN Vw_Preiodo32.ImpMov &gt;= 0    THEN Vw_Preiodo32.ImpMov 
   END  AS Periodo32     ,
   CASE 
      WHEN Vw_Preiodo33.ImpMov IS NULL THEN 0
      WHEN Vw_Preiodo33.ImpMov &gt;= 0    THEN Vw_Preiodo33.ImpMov 
   END  AS Periodo33     ,
   CASE 
      WHEN Vw_Preiodo34.ImpMov IS NULL THEN 0
      WHEN Vw_Preiodo34.ImpMov &gt;= 0    THEN Vw_Preiodo34.ImpMov 
   END  AS Periodo34     ,
   CASE 
      WHEN Vw_Preiodo35.ImpMov IS NULL THEN 0
      WHEN Vw_Preiodo35.ImpMov &gt;= 0    THEN Vw_Preiodo35.ImpMov 
   END  AS Periodo35     ,
   CASE 
      WHEN Vw_Preiodo36.ImpMov IS NULL THEN 0
      WHEN Vw_Preiodo36.ImpMov &gt;= 0    THEN Vw_Preiodo36.ImpMov 
   END  AS Periodo36     ,
   CASE 
      WHEN Vw_Preiodo37.ImpMov IS NULL THEN 0
      WHEN Vw_Preiodo37.ImpMov &gt;= 0    THEN Vw_Preiodo37.ImpMov 
   END  AS Periodo37     ,
   CASE 
      WHEN Vw_Preiodo38.ImpMov IS NULL THEN 0
      WHEN Vw_Preiodo38.ImpMov &gt;= 0    THEN Vw_Preiodo38.ImpMov 
   END  AS Periodo38     ,
   CASE 
      WHEN Vw_Preiodo39.ImpMov IS NULL THEN 0
      WHEN Vw_Preiodo39.ImpMov &gt;= 0    THEN Vw_Preiodo39.ImpMov 
   END  AS Periodo39     ,
   CASE 
      WHEN Vw_Preiodo40.ImpMov IS NULL THEN 0
      WHEN Vw_Preiodo40.ImpMov &gt;= 0    THEN Vw_Preiodo40.ImpMov 
   END  AS Periodo40        
  
FROM
   (   
				SELECT
		   Pry_Proyectos.IdProyecto                       ,
		   Pry_Proyectos.Codigo              AS CodProyect,
		   Pry_Proyectos.Nombre              AS NomProyect,
		   M_Paises.Nombre                   AS NomCountry,        
		   Org_Empresas.Nombre               AS NomEjecuta,
		   Pry_Proyectos.FechaInicio         AS PryFechini,    
		   Pry_Proyectos.FechaFin            AS PryFechFin,    
		   Obj.Descripcion                   AS Resultados,
		   Obj.IdObjetivo                    AS IdObjectiv,
		   Obj1.IdObjetivo                   AS IdProducto,
		   Obj1.Descripcion                  AS Producto,
		   Obj2.IdObjetivo                   AS IdActivity,
		   Obj2.Descripcion                  AS Actividads
		FROM
		   Pry_Proyectos INNER JOIN Pry_Objetivos            ON Pry_Objetivos.IdObjetivo  = Pry_Proyectos.IdProposito  
						 INNER JOIN Pry_Objetivos   AS Obj   ON Obj.IdPadre               = Pry_Objetivos.IdObjetivo 
						 INNER JOIN Pry_Objetivos   AS Obj1  ON Obj1.IdPadre              = Obj.IdObjetivo   
						  INNER JOIN Pry_Objetivos   AS Obj2 ON Obj2.IdPadre              = Obj1.IdObjetivo                 
						 INNER JOIN Org_Empresas             ON Pry_Proyectos.IdEmpresa   = Org_Empresas.IdEmpresa 
						 INNER JOIN M_Paises                 ON Org_Empresas.IdPais       = M_Paises.IdPais                  
 ) AS Vw_Proyecto 
	 LEFT JOIN  -- Presupuestos
	 (
	   SELECT
		   Pry_Proyectos.IdProyecto                   ,
		   Pry_Presupuesto.IdObjetivo                 ,
		   CASE
			 WHEN Sum(Pry_Presupuesto.Monto) IS NULL THEN 0
			 WHEN Sum(Pry_Presupuesto.Monto) &gt;=0     THEN Sum(Pry_Presupuesto.Monto)
		   END as Presupuest
	  FROM
		   Pry_Proyectos 
				INNER JOIN Pry_Presupuesto ON Pry_Proyectos.IdProyecto = Pry_Presupuesto.IdProyecto 
	  Group by Pry_Proyectos.IdProyecto,
	           Pry_Presupuesto.IdObjetivo 	
	 ) Vw_Presupus ON Vw_Proyecto.IdProyecto = Vw_Presupus.IdProyecto 
	              AND Vw_Proyecto.IdActivity = Vw_Presupus.IdObjetivo
	 LEFT JOIN  -- Periodo 01
	 (
		SELECT
		   Pry_Proyectos.IdProyecto                   ,
		   PRY_PERIODOSPROYECTOS.Nombre               ,
		   PRY_PERIODOSPROYECTOS.FechaInicio as FecIni,
		   PRY_PERIODOSPROYECTOS.FechaFin    as FecFin,
		   Vw_Movimientos.IdObjetivo                  ,
		   CASE
			 WHEN Sum(Vw_Movimientos.ImpPres) IS NULL THEN 0
			 WHEN Sum(Vw_Movimientos.ImpPres) &gt;=0     THEN Sum(Vw_Movimientos.ImpPres)
		   END as ImpPre,
		   CASE 
			 WHEN sum(Vw_Movimientos.ImpMonv) IS NULL THEN 0
			 WHEN sum(Vw_Movimientos.ImpMonv) &gt;= 0    THEN sum(Vw_Movimientos.ImpMonv)
		   END as ImpMov  
		FROM
		   Pry_Proyectos 
				INNER JOIN PRY_PERIODOSPROYECTOS ON Pry_Proyectos.IdProyecto = PRY_PERIODOSPROYECTOS.IdProyecto 
				LEFT  JOIN (                          
							SELECT 
							   Pry_Presupuesto.IdProyecto      ,
							   Pry_Presupuesto.IdPresupuesto   ,
							   Pry_Presupuesto.IdObjetivo      ,
							   Pry_Presupuesto.IdDonante       ,
							   Org_Donantes.Nombre   as Donante,   
							   Pry_Presupuesto.Monto as ImpPres,  
							   Pry_Movimientos.IdPeriodo       ,
							   Pry_Movimientos.Monto as ImpMonv        
							FROM 
							   Pry_Presupuesto 
								   LEFT JOIN Org_Donantes    ON Pry_Presupuesto.IdDonante     = Org_Donantes.IdDonante 
								   LEFT JOIN Pry_Movimientos ON Pry_Presupuesto.IdPresupuesto = Pry_Movimientos.IdPresupuesto
						  ) Vw_Movimientos ON Pry_Proyectos.IdProyecto        = Vw_Movimientos.IdProyecto
										  AND PRY_PERIODOSPROYECTOS.IdPeriodo = Vw_Movimientos.IdPeriodo    
         WHERE PRY_PERIODOSPROYECTOS.Nombre = 'Periodo 1'										  
		 GROUP BY Pry_Proyectos.IdProyecto         ,
				  PRY_PERIODOSPROYECTOS.Nombre     ,               
				  PRY_PERIODOSPROYECTOS.FechaInicio, 
				  PRY_PERIODOSPROYECTOS.FechaFin   ,
				   Vw_Movimientos.IdObjetivo
       ) as Vw_Preiodo01 ON  Vw_Proyecto.IdProyecto = Vw_Preiodo01.IdProyecto 
                         AND Vw_Proyecto.IdActivity = Vw_Preiodo01.IdObjetivo
	  LEFT JOIN  -- Periodo 02
	  (
		SELECT
		   Pry_Proyectos.IdProyecto                   ,
		   PRY_PERIODOSPROYECTOS.Nombre               ,
		   PRY_PERIODOSPROYECTOS.FechaInicio as FecIni,
		   PRY_PERIODOSPROYECTOS.FechaFin    as FecFin,
		   Vw_Movimientos.IdObjetivo                  ,
		   CASE
			 WHEN Sum(Vw_Movimientos.ImpPres) IS NULL THEN 0
			 WHEN Sum(Vw_Movimientos.ImpPres) &gt;=0     THEN Sum(Vw_Movimientos.ImpPres)
		   END as ImpPre,
		   CASE 
			 WHEN sum(Vw_Movimientos.ImpMonv) IS NULL THEN 0
			 WHEN sum(Vw_Movimientos.ImpMonv) &gt;= 0    THEN sum(Vw_Movimientos.ImpMonv)
		   END as ImpMov  
		FROM
		   Pry_Proyectos 
				INNER JOIN PRY_PERIODOSPROYECTOS ON Pry_Proyectos.IdProyecto = PRY_PERIODOSPROYECTOS.IdProyecto 
				LEFT  JOIN (                          
							SELECT 
							   Pry_Presupuesto.IdProyecto      ,
							   Pry_Presupuesto.IdPresupuesto   ,
							   Pry_Presupuesto.IdObjetivo      ,
							   Pry_Presupuesto.IdDonante       ,
							   Org_Donantes.Nombre   as Donante,   
							   Pry_Presupuesto.Monto as ImpPres,  
							   Pry_Movimientos.IdPeriodo       ,
							   Pry_Movimientos.Monto as ImpMonv        
							FROM 
							   Pry_Presupuesto 
								   LEFT JOIN Org_Donantes    ON Pry_Presupuesto.IdDonante     = Org_Donantes.IdDonante 
								   LEFT JOIN Pry_Movimientos ON Pry_Presupuesto.IdPresupuesto = Pry_Movimientos.IdPresupuesto
						  ) Vw_Movimientos ON Pry_Proyectos.IdProyecto        = Vw_Movimientos.IdProyecto
										  AND PRY_PERIODOSPROYECTOS.IdPeriodo = Vw_Movimientos.IdPeriodo    
         WHERE PRY_PERIODOSPROYECTOS.Nombre = 'Periodo 2'										  
		 GROUP BY Pry_Proyectos.IdProyecto         ,
				  PRY_PERIODOSPROYECTOS.Nombre     ,               
				  PRY_PERIODOSPROYECTOS.FechaInicio, 
				  PRY_PERIODOSPROYECTOS.FechaFin   ,
				   Vw_Movimientos.IdObjetivo
       ) as Vw_Preiodo02 ON  Vw_Proyecto.IdProyecto = Vw_Preiodo02.IdProyecto 
                         AND Vw_Proyecto.IdActivity = Vw_Preiodo02.IdObjetivo
	  LEFT JOIN  -- Periodo 03
	  (
		SELECT
		   Pry_Proyectos.IdProyecto                   ,
		   PRY_PERIODOSPROYECTOS.Nombre               ,
		   PRY_PERIODOSPROYECTOS.FechaInicio as FecIni,
		   PRY_PERIODOSPROYECTOS.FechaFin    as FecFin,
		   Vw_Movimientos.IdObjetivo                  ,
		   CASE
			 WHEN Sum(Vw_Movimientos.ImpPres) IS NULL THEN 0
			 WHEN Sum(Vw_Movimientos.ImpPres) &gt;=0     THEN Sum(Vw_Movimientos.ImpPres)
		   END as ImpPre,
		   CASE 
			 WHEN sum(Vw_Movimientos.ImpMonv) IS NULL THEN 0
			 WHEN sum(Vw_Movimientos.ImpMonv) &gt;= 0    THEN sum(Vw_Movimientos.ImpMonv)
		   END as ImpMov  
		FROM
		   Pry_Proyectos 
				INNER JOIN PRY_PERIODOSPROYECTOS ON Pry_Proyectos.IdProyecto = PRY_PERIODOSPROYECTOS.IdProyecto 
				LEFT  JOIN (                          
							SELECT 
							   Pry_Presupuesto.IdProyecto      ,
							   Pry_Presupuesto.IdPresupuesto   ,
							   Pry_Presupuesto.IdObjetivo      ,
							   Pry_Presupuesto.IdDonante       ,
							   Org_Donantes.Nombre   as Donante,   
							   Pry_Presupuesto.Monto as ImpPres,  
							   Pry_Movimientos.IdPeriodo       ,
							   Pry_Movimientos.Monto as ImpMonv        
							FROM 
							   Pry_Presupuesto 
								   LEFT JOIN Org_Donantes    ON Pry_Presupuesto.IdDonante     = Org_Donantes.IdDonante 
								   LEFT JOIN Pry_Movimientos ON Pry_Presupuesto.IdPresupuesto = Pry_Movimientos.IdPresupuesto
						  ) Vw_Movimientos ON Pry_Proyectos.IdProyecto        = Vw_Movimientos.IdProyecto
										  AND PRY_PERIODOSPROYECTOS.IdPeriodo = Vw_Movimientos.IdPeriodo    
         WHERE PRY_PERIODOSPROYECTOS.Nombre = 'Periodo 3'										  
		 GROUP BY Pry_Proyectos.IdProyecto         ,
				  PRY_PERIODOSPROYECTOS.Nombre     ,               
				  PRY_PERIODOSPROYECTOS.FechaInicio, 
				  PRY_PERIODOSPROYECTOS.FechaFin   ,
				   Vw_Movimientos.IdObjetivo
       ) as Vw_Preiodo03 ON  Vw_Proyecto.IdProyecto = Vw_Preiodo03.IdProyecto 
                         AND Vw_Proyecto.IdActivity = Vw_Preiodo03.IdObjetivo        
	  LEFT JOIN  -- Periodo 04
	  (
		SELECT
		   Pry_Proyectos.IdProyecto                   ,
		   PRY_PERIODOSPROYECTOS.Nombre               ,
		   PRY_PERIODOSPROYECTOS.FechaInicio as FecIni,
		   PRY_PERIODOSPROYECTOS.FechaFin    as FecFin,
		   Vw_Movimientos.IdObjetivo                  ,
		   CASE
			 WHEN Sum(Vw_Movimientos.ImpPres) IS NULL THEN 0
			 WHEN Sum(Vw_Movimientos.ImpPres) &gt;=0     THEN Sum(Vw_Movimientos.ImpPres)
		   END as ImpPre,
		   CASE 
			 WHEN sum(Vw_Movimientos.ImpMonv) IS NULL THEN 0
			 WHEN sum(Vw_Movimientos.ImpMonv) &gt;= 0    THEN sum(Vw_Movimientos.ImpMonv)
		   END as ImpMov  
		FROM
		   Pry_Proyectos 
				INNER JOIN PRY_PERIODOSPROYECTOS ON Pry_Proyectos.IdProyecto = PRY_PERIODOSPROYECTOS.IdProyecto 
				LEFT  JOIN (                          
							SELECT 
							   Pry_Presupuesto.IdProyecto      ,
							   Pry_Presupuesto.IdPresupuesto   ,
							   Pry_Presupuesto.IdObjetivo      ,
							   Pry_Presupuesto.IdDonante       ,
							   Org_Donantes.Nombre   as Donante,   
							   Pry_Presupuesto.Monto as ImpPres,  
							   Pry_Movimientos.IdPeriodo       ,
							   Pry_Movimientos.Monto as ImpMonv        
							FROM 
							   Pry_Presupuesto 
								   LEFT JOIN Org_Donantes    ON Pry_Presupuesto.IdDonante     = Org_Donantes.IdDonante 
								   LEFT JOIN Pry_Movimientos ON Pry_Presupuesto.IdPresupuesto = Pry_Movimientos.IdPresupuesto
						  ) Vw_Movimientos ON Pry_Proyectos.IdProyecto        = Vw_Movimientos.IdProyecto
										  AND PRY_PERIODOSPROYECTOS.IdPeriodo = Vw_Movimientos.IdPeriodo    
         WHERE PRY_PERIODOSPROYECTOS.Nombre = 'Periodo 4'										  
		 GROUP BY Pry_Proyectos.IdProyecto         ,
				  PRY_PERIODOSPROYECTOS.Nombre     ,               
				  PRY_PERIODOSPROYECTOS.FechaInicio, 
				  PRY_PERIODOSPROYECTOS.FechaFin   ,
				   Vw_Movimientos.IdObjetivo
       ) as Vw_Preiodo04 ON  Vw_Proyecto.IdProyecto = Vw_Preiodo04.IdProyecto 
                         AND Vw_Proyecto.IdActivity = Vw_Preiodo04.IdObjetivo  
	  LEFT JOIN  -- Periodo 05
	  (
		SELECT
		   Pry_Proyectos.IdProyecto                   ,
		   PRY_PERIODOSPROYECTOS.Nombre               ,
		   PRY_PERIODOSPROYECTOS.FechaInicio as FecIni,
		   PRY_PERIODOSPROYECTOS.FechaFin    as FecFin,
		   Vw_Movimientos.IdObjetivo                  ,
		   CASE
			 WHEN Sum(Vw_Movimientos.ImpPres) IS NULL THEN 0
			 WHEN Sum(Vw_Movimientos.ImpPres) &gt;=0     THEN Sum(Vw_Movimientos.ImpPres)
		   END as ImpPre,
		   CASE 
			 WHEN sum(Vw_Movimientos.ImpMonv) IS NULL THEN 0
			 WHEN sum(Vw_Movimientos.ImpMonv) &gt;= 0    THEN sum(Vw_Movimientos.ImpMonv)
		   END as ImpMov  
		FROM
		   Pry_Proyectos 
				INNER JOIN PRY_PERIODOSPROYECTOS ON Pry_Proyectos.IdProyecto = PRY_PERIODOSPROYECTOS.IdProyecto 
				LEFT  JOIN (                          
							SELECT 
							   Pry_Presupuesto.IdProyecto      ,
							   Pry_Presupuesto.IdPresupuesto   ,
							   Pry_Presupuesto.IdObjetivo      ,
							   Pry_Presupuesto.IdDonante       ,
							   Org_Donantes.Nombre   as Donante,   
							   Pry_Presupuesto.Monto as ImpPres,  
							   Pry_Movimientos.IdPeriodo       ,
							   Pry_Movimientos.Monto as ImpMonv        
							FROM 
							   Pry_Presupuesto 
								   LEFT JOIN Org_Donantes    ON Pry_Presupuesto.IdDonante     = Org_Donantes.IdDonante 
								   LEFT JOIN Pry_Movimientos ON Pry_Presupuesto.IdPresupuesto = Pry_Movimientos.IdPresupuesto
						  ) Vw_Movimientos ON Pry_Proyectos.IdProyecto        = Vw_Movimientos.IdProyecto
										  AND PRY_PERIODOSPROYECTOS.IdPeriodo = Vw_Movimientos.IdPeriodo    
