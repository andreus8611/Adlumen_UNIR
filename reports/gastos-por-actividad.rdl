    <DataSet Name="Proyectos">
      <Query>
        <DataSourceName>RsSource01</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="IdTenant">
            <Value>=Parameters!IdTenant.Value</Value>
            <rd:UserDefined>true</rd:UserDefined>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT  DISTINCT
     Pry_Proyectos.IdEmpresa,
     Pry_Proyectos.IdProyecto,
     upper(Pry_Proyectos.Nombre)     as nombre,
    Vw_Empresas.Nombre  
from
  dbo.Pry_Proyectos INNER JOIN 
  (SELECT 
	  Org_Empresas.IdEmpresa          ,
	  Org_Empresas.Nombre             ,
	  Sys_Usuarios_Empresas.IdUsuario , 
	  Sys_Usuarios.Nombre as username ,
	  Sys_Usuarios.UserName  UserLogin,
	  Sys_Usuarios.UserReport
	from
	  Sys_Usuarios_Empresas 
		 inner join dbo.Sys_Usuarios ON Sys_Usuarios_Empresas.IdUsuario = Sys_Usuarios.IdUsuario 
		 inner join dbo.Org_Empresas             ON Org_Empresas.IdEmpresa          = Sys_Usuarios_Empresas.IdEmpresa   
  ) as Vw_Empresas ON Pry_Proyectos.IdEmpresa  = Vw_Empresas.IdEmpresa
WHERE Pry_Proyectos.IdTenant = @IdTenant   and
Pry_Proyectos.Eliminado =0</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="IdEmpresa">
          <DataField>IdEmpresa</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="IdProyecto">
          <DataField>IdProyecto</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="nombre">
