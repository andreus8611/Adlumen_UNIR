   Vw_Proyecto.Resultados,
   Vw_Proyecto.PryFechini,
   Vw_Proyecto.PryFechFin,
    Vw_Proyecto.IdProducto,
   Vw_Proyecto.Producto,
   Vw_Proyecto.IdActivity,
   Vw_Proyecto.Actividads,
   Vw_Presupus.Presupuest,
   CASE 
      WHEN Vw_Preiodo01.ImpMov IS NULL THEN 0
      WHEN Vw_Preiodo01.ImpMov &gt;= 0    THEN Vw_Preiodo01.ImpMov 
   END  AS Periodo01     ,
   CASE 
      WHEN Vw_Preiodo02.ImpMov IS NULL THEN 0
