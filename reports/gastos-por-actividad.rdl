    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>RsSource01</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@ParmProyectoID">
            <Value>=Parameters!ParmProyectoID.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>Select  pObj.IdProyecto,
 (select Descripcion  from Pry_Objetivos  where IdObjetivo=  (select IdPadre  from Pry_Objetivos  where IdObjetivo=pObj.IdPadre)) Resultados
 , (select Descripcion  from Pry_Objetivos  where IdObjetivo=pObj.IdPadre) Producto,
  pObj.IdObjetivo Actividad, pObj.Descripcion, periodo.IdPeriodo, year(periodo.FechaFin) anio, periodo.nombre,
 ( SELECT p.Monto FROM Pry_Presupuesto  P
WHERE P.IdProyecto=@ParmProyectoID AND P.IdObjetivo=  pObj.IdObjetivo),
 (select COUNT(per.IdPeriodo) total  from  Pry_Objetivos o,  PRY_PERIODOSPROYECTOS per 
where o.IdObjetivo= pObj.IdObjetivo and per.FechaInicio&gt;=convert(date,o.FechaInicio)
   and per.FechaFin&lt;=DATEADD(DAY,1,o.FechaFin) and per.IdProyecto=@ParmProyectoID
  ) Periodos , 
 round(  ( SELECT p.Monto FROM Pry_Presupuesto  P
WHERE P.IdProyecto=@ParmProyectoID AND P.IdObjetivo=  pObj.IdObjetivo) / 
(select COUNT(per.IdPeriodo) total  from  Pry_Objetivos o,  PRY_PERIODOSPROYECTOS per 
where o.IdObjetivo= pObj.IdObjetivo and per.FechaInicio&gt;=convert(date,o.FechaInicio)
   and per.FechaFin&lt;=DATEADD(DAY,1,o.FechaFin) and per.IdProyecto=@ParmProyectoID
  ),1) Presupuesto ,
  
  (select SUM(APORTEPROGRAMA)  from Pry_Movimientos where IdPresupuesto=(select IdPresupuesto
  from Pry_Presupuesto where IdObjetivo=pObj.IdObjetivo )
     and IdPeriodo=periodo.IdPeriodo ) Ejercido ,
     
     
      round(  ( SELECT p.Monto FROM Pry_Presupuesto  P
WHERE P.IdProyecto=1218 AND P.IdObjetivo=  pObj.IdObjetivo) / 
(select COUNT(per.IdPeriodo) total  from  Pry_Objetivos o,  PRY_PERIODOSPROYECTOS per 
where o.IdObjetivo= pObj.IdObjetivo and per.FechaInicio&gt;=convert(date,o.FechaInicio)
   and per.FechaFin&lt;=DATEADD(DAY,1,o.FechaFin) and per.IdProyecto=@ParmProyectoID
  )  - 
  
  (select SUM(APORTEPROGRAMA)  from Pry_Movimientos where IdPresupuesto=(select IdPresupuesto
  from Pry_Presupuesto where IdObjetivo=pObj.IdObjetivo )
     and IdPeriodo=periodo.IdPeriodo ),1) Saldo
     

from Pry_Objetivos pObj ,Pry_PeriodosProyectos periodo
 where pObj.IdProyecto=@ParmProyectoID and pObj.IdObjetivoClase=5 and periodo.FechaInicio&gt;=convert(date,pObj.FechaInicio)
   and periodo.FechaFin&lt;=DATEADD(DAY,1,pObj.FechaFin) and periodo.IdProyecto=@ParmProyectoID

order by pObj.Descripcion</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
